#!/bin/csh -f
# 親OzDomainResolver設定ファイル

#以下を設定
 setenv CLASSPATH $CLASSPATH:Resolver.zipのパス/Resolver.zip
 set domain=ドメイン名
 set port=ポート番号
 set apgwAddr=ApplicaionGateWayのIPアドレス:ポート番号

#例
#setenv CLASSPATH $CLASSPATH:$OZOME/lib/Resolver.zip
#set domain=oz.ipa.go.jp
#set port=9000
#set apgwAddr=192.170.99.90:10000

#以下は改編不可
java JP.go.ipa.oz.user.res.OzDomainResolver $domain $port $apgwAddr 
